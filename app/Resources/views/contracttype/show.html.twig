{% extends 'base.html.twig' %}

{% block body %}
    <h1>Contracttype</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ contractType.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ contractType.name }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('contracttype_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('contracttype_edit', { 'id': contractType.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
